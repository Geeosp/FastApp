<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.geeo.fastapp.fastapp" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OneActivity"
            android:label="@string/title_activity_one" >
        </activity>
        <activity
            android:name=".TwoActivity"
            android:label="@string/title_activity_two" >
        </activity>
        <activity
            android:name=".SimonActivity"
            android:label="@string/title_activity_simon" >
        </activity>
        <activity
            android:name=".VelhaActivity"
            android:label="@string/title_activity_velha" >
        </activity>
    </application>

</manifest>
